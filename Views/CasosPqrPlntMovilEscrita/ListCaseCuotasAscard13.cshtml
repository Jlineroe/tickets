@model IEnumerable<AIBTicketsMVC.Models.CasosPqrMovilEscrita>

<table id="TableGeneral4" class="table table-sm table-bordered table-hover" style="width:100%;">
    <thead>
        <tr>
            <th></th>
            <th>IdSolutions</th>
            <th>Numero Credito</th>
            <th>Valor nueva cuota</th>
            <th>Ascard2_Concepto</th>
            <th>Ascard2_Área_soliciita_ajuste</th>
            <th>Ascard2_Reclamo_Usuario</th>
            <th>Ascard2_Cantidad_cuotas</th>
            <th>Ascard2_Aliado</th>
            <th>Ascard2_Estado</th>
            <th>Ascard2_Ultima_Actualizacion</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td></td>
            <td>@item.WorKOrder</td>
            <td>@item.Ascard2_Numero_de_credito_V</td>
            <td>@item.Ascard2_Valor_Nueva_Cuota_V</td>
            <td>@item.Ascard2_Concepto_V</td>
            <td>@item.Ascard2_Área_soliciita_ajuste_V</td>
            <td>@item.Ascard2_Reclamo_Usuario_V</td>
            <td>@item.Ascard2_Cantidad_cuotas_V</td>
            <td>@item.Ascard2_Aliado_V</td>
            <td>@item.Ascard2_Estado_V</td>
            <td>@item.Ascard2_Ultima_Actualizacion_V</td>
           

        </tr>
        }

    </tbody>
</table>

