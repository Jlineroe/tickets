@model IEnumerable<AIBTicketsMVC.Models.CasosPqrMovilEscrita>

<table id="TableGeneral2" class="table table-sm table-bordered table-hover" style="width:100%;">
    <thead>
        <tr>
            <th></th>
            <th>SOLUTIONS</th>
            <th>MIN</th>
            <th>CUN</th>
            <th>BPB_CUSTCODE</th>
            <th>BPB_CUSTOMER_ID</th>
            <th>BPB_AREA_QUE_GENERO_INCONSISTENCIA</th>
            <th>BPB_USUARIO_RESPONSABLE</th>
            <th>BPB_ANALISTA</th>
            <th>BPB_USER_RED</th>
            <th>BPB_TIPO_RECLAMO</th>
            <th>BPB_Valor</th>
            <th>BPB_Justificación</th>
            <th>BPB_Periodo_ajustar_desde</th>
            <th>BPB_Periodo_ajustar_Hasta</th>
            <th>BPB_SERVICIO</th>
            <th>BPB_CTA_CONTABLE</th>
            <th>BPB_IVA</th>
            <th>BPB_GERENCIA</th>
            <th>BPB_CENTRALES</th>
            <th>BPB_ALIADO</th>
            <th>BPB_CAUSAL</th>
            <th>BPB_ESTADO</th>
            @*<th>BPB_ACTUALIZACION_ANTERIOR</th>*@
            <th>BPB_ULTIMA_ACTUALIZACION</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td></td>
            <td>@item.WorKOrder</td>
            <td>@item.BPB_MIN</td>
            <td>@item.BPB_CUN</td>
            <td>@item.BPB_CUSTCODE</td>
            <td>@item.BPB_CUSTOMER_ID</td>
            <td>@item.BPB_AREA_QUE_GENERO_INCONSISTENCIA</td>
            <td>@item.BPB_USUARIO_RESPONSABLE</td>
            <td>@item.BPB_ANALISTA</td>
            <td>@item.BPB_USER_RED</td>
            <td>@item.BPB_TIPO_RECLAMO</td>
            <td>@item.BPB_Valor</td>
            <td>@item.BPB_Justificación</td>
            <td>@item.BPB_Periodo_ajustar_desde</td>
            <td>@item.BPB_Periodo_ajustar_Hasta</td>
            <td>@item.BPB_SERVICIO</td>
            <td>@item.BPB_CTA_CONTABLE</td>
            <td>@item.BPB_IVA</td>
            <td>@item.BPB_GERENCIA</td>
            <td>@item.BPB_CENTRALES</td>
            <td>@item.BPB_ALIADO</td>
            <td>@item.BPB_CAUSAL</td>
            <td>@item.BPB_ESTADO</td>
            @*<td>@item.BPB_ACTUALIZACION_ANTERIOR</td>*@
            <td>@item.BPB_ULTIMA_ACTUALIZACION</td>
           

        </tr>
        }

    </tbody>
</table>

