@model AIBTicketsMVC.Models.ListasDesplegables

<div class="form-group row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <ul class="list-group">
            <li class="list-group-item list-group-item-primary">
                Resumen de la importacion
            </li>
            <li class="list-group-item d-flex justify-content-between align-items-center">
                Numero total de registros
                <span id="divNRegisTotal" class="badge badge-primary badge-pill"></span>
            </li>
        </ul>
    </div>
</div>
<form method="post" id="formFinalizar">
    <div class="form-group row">
        <div class="col-md-3">
            <label>Grupo a asignar</label>
            <select class="form-control required" id="DdlGroups" onchange="onChangeGroups_ListUsers(this.value);">
                <option value="">--Seleccionar--</option>
            </select>
        </div>
        <div class="col-md-3">
            <label>Usuarios</label>
            <select data-placeholder="Seleccione usuarios..." multiple class="chosen-select" id="DdlUsuarios">
            </select>
        </div>
        <div class="col-md-3">
            <label>Algoritmo</label>
            <select class="form-control required" id="DdlAlgoritmo">
                <option value="">--Seleccionar--</option>
                @foreach (var item in Model.Algorithms)
                {
                    <option value="@item.IdAlgorithmsAssignment">@item.NameAlgorithm</option>
                }
            </select>
        </div>
        <div class="col-md-3">
            <input id="BtnFinalizar" type="submit" value="Finalizar Importacion" class="btn btn-primary" style="margin-top:30px;" />
            <button class="btn btn-primary BtnLoading" type="submit" style="display:none;margin-top:30px;" disabled>
                <span class="spinner-border spinner-border-sm"></span>
                Loading...
            </button>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md">
            <b id="Importar-error" class="text-danger"></b>
        </div>
    </div>
</form>
