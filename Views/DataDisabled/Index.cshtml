@model IEnumerable<AIBTicketsMVC.Models.WorkOrder_DataImported>
@{
    ViewData["Title"] = "Desactivar datas";
}

@section Styles {
    <link href="~/wwwroot/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/wwwroot/vendor/sweetalert/sweetalert.css" rel="stylesheet" />
    <style type="text/css">
        th, td {
            white-space: nowrap;
        }
    </style>
}

@section Scripts {
    <script src="~/wwwroot/vendor/sweetalert/sweetalert.min.js"></script>
    <script src="~/wwwroot/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/wwwroot/vendor/datatables/dataTables.bootstrap4.min.js"></script>
    <script src="~/wwwroot/assets/js/Forms/UtilitiesJS.js"></script>
    <script src="~/wwwroot/assets/js/Forms/DataDisabled.js"></script>
}

<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Categorias</h1>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">
                <h6 class="m-0 font-weight-bold text-primary">Datas cargadas</h6>
            </div>
            <div class="card-body">
                <div class="form-group row">
                    <div class="table-responsive" id="divTable">
                        @Html.Partial("ListDataImport", Model)
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>